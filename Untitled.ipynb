    "import csv\n",
    "\n",
    "d = dict()\n",
    "with open('adult.csv') as csvfile:\n",
    "  # 解析內容\n",
    "  rows = list(csv.reader(csvfile))\n",
    "  for row in rows[1:]:\n",
    "    # print(row[2][:3], row[4])\n",
    "    if row[2][:3] not in d:\n",
    "      d[row[2][:3]] = 0\n",
    "    d[row[2][:3]] = d[row[2][:3]] + int(row[4])\n",
    "\n",
    "print(d)"
   ]
  }
